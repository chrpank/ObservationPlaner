	$(PYTHON) $(SCRIPT) $(PARA_SITE) $(PARA_JUPI) $(DATA_JUPI_2026) $(DATA_SUNN_2026)
	$(PYTHON) $(SCRIPT) $(PARA_SITE) $(PARA_JUPI) $(DATA_JUPI_2027) $(DATA_SUNN_2027)
	$(PYTHON) $(SCRIPT) $(PARA_SITE) $(PARA_JUPI) $(DATA_JUPI_2028) $(DATA_SUNN_2028)
